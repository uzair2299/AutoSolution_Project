@model AutoSolution.Services.ViewModel.PartsProductsViewModel


<div class="modal fade" id="kt_modal_Details" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Vehicle Version Detail</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                </button>
            </div>
            <div class="modal-body">
                <div class="col-md-12 col-lg-12 col-xl-4">
                    <!--begin:: Widgets/Stats2-1 -->
                    <div class="kt-widget1">
                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.PartsProductName)</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.PartsProductName)</span>
                            </div>
                        </div>
                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayName("Year")</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.startYear) - @Html.DisplayFor(model => model.EndYear)</span>
                            </div>
                        </div>
                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.UnitPrice)</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.UnitPrice)</span>
                            </div>
                        </div>
                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.VehicleManufacturerName)</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.VehicleManufacturerName)</span>
                            </div>
                        </div>
                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.VehicleModelName)</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.VehicleModelName)</span>
                            </div>
                        </div>
                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.PartsProductCategoryName)</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.PartsProductCategoryName)</span>
                            </div>
                        </div>

                        <div class="kt-widget1__item">
                            <div class="kt-widget1__info">
                                <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.PartsProductSubCategoryName)</h3>
                                <span class="kt-widget1__desc">@Html.DisplayFor(model => model.PartsProductSubCategoryName)</span>
                            </div>
                        </div>

                        <div class="kt-widget1__item">
                                <div class="kt-widget1__info">
                                    <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.PartsProductManuName)</h3>
                                    <span class="kt-widget1__desc">@Html.DisplayFor(model => model.PartsProductManuName)</span>
                                </div>
                            </div>
                        @*<div class="kt-widget1__item">
                                <div class="kt-widget1__info">
                                    <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.UpdateDate)</h3>
                                    <span class="kt-widget1__desc">@Html.DisplayFor(model => model.UpdateDate)</span>
                                </div>
                            </div>*@
                        @*<div class="kt-widget1__item">
                                <div class="kt-widget1__info">
                                    @{ string str = null;

                                        if (Model.IsActive == true)
                                        {
                                            str = "Active";
                                        }
                                        else
                                        {
                                            str = "Not Active";
                                        }
                                    }
                                    <h3 class="kt-widget1__title">@Html.DisplayNameFor(model => model.IsActive)</h3>
                                    <span class="kt-widget1__desc">@str</span>
                                </div>
                            </div>*@
                    </div>

                    <!--end:: Widgets/Stats2-1 -->
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>


